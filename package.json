{
  "name": "pcbeebackend",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/index.js",
    "test": "jest",
    "build": "tsc",
    "db:migrate:reset": "npx supabase db reset && npx drizzle-kit migrate && tsx ./drizzle/seed.ts",
    "db:migrate:seed": "npx drizzle-kit migrate && tsx ./drizzle/seed.ts",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:generate": "drizzle-kit generate",
    "db:reset": "npx supabase db reset",
    "db:generate:drop": "drizzle-kit drop",
    "db:migrate": "npx drizzle-kit migrate",
    "db:studio": "npx drizzle-kit studio",
    "db:seed": "tsx ./drizzle/seed.ts",
    "prepare": "husky install && npx lint-staged",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "keywords": [
    "NodeJs",
    "Express",
    "drizzle",
    "API"
  ],
  "author": "Khesir",
  "license": "ISC",
  "description": "PC BEE backend",
  "devDependencies": {
    "@eslint/js": "9.9.1",
    "@types/cors": "2.8.17",
    "@types/eslint__js": "8.42.3",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/mysql": "2.15.26",
    "@types/supertest": "6.0.2",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.6",
    "@typescript-eslint/eslint-plugin": "8.3.0",
    "@typescript-eslint/parser": "8.3.0",
    "drizzle-kit": "0.24.2",
    "eslint": "9.9.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "globals": "15.9.0",
    "husky": "8.0.0",
    "jest": "29.7.0",
    "lint-staged": "15.2.9",
    "nodemon": "3.1.4",
    "prettier": "3.3.3",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript-eslint": "8.3.0"
  },
  "dependencies": {
    "@faker-js/faker": "8.4.1",
    "@supabase/supabase-js": "^2.45.4",
    "@types/pg": "^8.11.10",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "drizzle-orm": "0.33.0",
    "express": "4.21.0",
    "mysql2": "3.11.0",
    "pcbeebackend": "file:",
    "pino": "9.3.2",
    "pino-pretty": "11.2.2",
    "postgres": "^3.4.4",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.19.0",
    "zod": "3.23.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "forceExit": true
  }
}
