{
  "name": "pcbeebackend",
  "version": "1.0.0",
  "main": "src/main.ts",
  "scripts": {
    "start:dev": "nodemon src/main.ts",
    "start:prod": "node src/main.ts",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:generate": "drizzle-kit generate",
    "db:generate:drop": "drizzle-kit drop",
    "db:migrate": "npx tsx drizzle/migrate",
    "db:studio": "npx drizzle-kit studio",
    "prepare": "husky install && npx lint-staged",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "keywords": [
    "NodeJs",
    "Express",
    "prisma",
    "API"
  ],
  "author": "Khesir",
  "license": "ISC",
  "description": "PC BEE backend",
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "4.17.21",
    "@types/ip": "1.1.3",
    "@types/mysql": "2.15.26",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "husky": "8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "nodemon": "3.1.4",
    "prettier": "^3.3.3",
    "ts-node": "10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0"
  },
  "dependencies": {
    "cors": "2.8.5",
    "dockernodeprismapostgress": "file:",
    "dotenv": "16.4.5",
    "drizzle-orm": "^0.33.0",
    "express": "4.19.2",
    "ip": "2.0.1",
    "mysql2": "3.11.0",
    "pcbeebackend": "file:",
    "pino": "9.3.2",
    "pino-pretty": "11.2.2",
    "postgres": "^3.4.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
